<view class="container legend-list">
  <block wx:for="{{record}}" wx:key="time">
    <view class="flex-item">
    <view class="image-section">
    <image style="width:80px;height:80px;background-color:#eeeeee;"
        src="{{item.img}}" mode="aspectFit"></image>
    </view>
    <view class="item-right">
    <view><text class="legend-item1">{{item.time}}</text></view>
    <view><text class="legend-item1">{{item.kda}}</text></view>
    </view>
    </view>
  </block>
</view>
